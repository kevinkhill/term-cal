{
  "name": "@khill/term-cal",
  "description": "Calendar for your terminal.",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "main": "dist/main.cjs.js",
  "types": "dist/main.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "node ./demo.js",
    "dev": "tsnd --respawn --clear ./src/main.ts",
    "lint": "ts-engine lint",
    "test": "ts-engine test",
    "typecheck": "ts-engine typecheck",
    "build": "yarn build:lib && yarn build:types",
    "build:lib": "ts-engine build --library",
    "build:types": "ts-engine typecheck --emit"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "date-fns": "^2.14.0",
    "ts-pattern": "^1.1.0"
  },
  "devDependencies": {
    "@ts-engine/cli": "^1.10.0",
    "ts-node-dev": "^1.0.0-pre.56",
    "ultra-runner": "^3.5.0"
  },
  "ultra": {
    "concurrent": [
      "build"
    ]
  }
}
